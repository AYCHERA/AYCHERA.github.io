"items": [{% assign skemas = assets %}{% assign my_array = "" | split: ',' %}{% for skema in skemas %}{% assign my_path = section.title | append: '/' | append:skema.title | downcase %}{% if skema.path contains my_path %}{% assign my_array = my_array | push: skema %}{% endif %}{% endfor %}{% for item in my_array %}
                {
                    "title": {{ item.path }},
                }{% unless forloop.last %},{% endunless %}{% endfor %}
            ]
