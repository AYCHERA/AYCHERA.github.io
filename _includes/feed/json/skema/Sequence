"items": [{% assign items = assets %}{% assign my_array = "" | split: ',' %}{% for item in items %}{% assign my_path = result.title | append: '/' | append:item.title | downcase %}{% if item.path contains my_path %}{% assign my_array = my_array | push: item %}{% endif %}{% endfor %}{% assign results = my_array %}{% for result in results %}{% assign section = result %}
                        {
                            "title": {{ section.title | jsonify }},
                            "include": {{ my_path | jsonify }}
                        }{% unless forloop.last %},{% endunless %}{% endfor %}
                    ]
